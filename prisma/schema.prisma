generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Link {
    id    Int    @id @default(autoincrement())
    url   String
    alias String @unique @db.VarChar(6)

    @@index(name: "alias", fields: [alias])
}
